      <bpmn2:script><![CDATA[String dominio = "https://restguardians.westeurope.cloudapp.azure.com:8080/guardians/api/";

String URICalendario=dominio+"calendars/";

kcontext.setVariable("URICalendarios",URICalendario);

String headers = "Method=GET;Authorization=Basic Z3VhcmRpYW5zVXNlcjpkcUZ0TkRzYXZGdVFUNGZqSEJxag==;Accept=*/*;Accept-Encoding=gzip,deflate,br;Connection=keep-alive;Cookie=JSESSIONID=5F8F4C002FFFA28691A21D8332EA97DA";
kcontext.setVariable("HEADERS",headers);

java.util.Calendar c1 = java.util.Calendar.getInstance();
int month = c1.get(java.util.Calendar.MONTH)+1;
int year = c1.get(java.util.Calendar.YEAR);

